{
   "compilerOptions": {
      "experimentalDecorators": true,
      "useDefineForClassFields": true,
      "strict": true,
      "module": "NodeNext",
      "moduleResolution": "NodeNext",
      "noEmit": false,
      "noEmitOnError": false,
      "target": "es6",
      "esModuleInterop": true,
      "sourceMap": true,
      "rootDir": "src",
      "outDir": "dist/js",
      "typeRoots": [ "node_modules/@types" ],
      "lib": [ "ESNext", "DOM" ],
      "noImplicitAny":false
   }
}